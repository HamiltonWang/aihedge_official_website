    <!-- Start Page Title Area -->
    <div class="page-title-area item-bg3 jarallax">
        <div class="container">
            <div class="page-title-content">
                <h1 class="lang" key="Trademark"></h1>
                <h4 class="lang" key="AI Hedge / AI Art Trademark Guidelines"></h4>
            </div>
